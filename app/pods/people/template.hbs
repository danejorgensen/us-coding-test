<div class="row points">
  <div class="col s12">
    <h4 class="center-align">
      <img class="logo" src="/images/logo.png" alt="U.S. Ski and Snowboard logo">
      U.S. Ski and Snowboard Coding Test
    </h4>
  </div>
  <div class="col s12">
    <table class="list striped">
      <tbody>
        {{#each this.model as |person| }}
          <tr>
            <div class="row">
              <Details @person={{person}} />
              <Address @person={{person}} />
              <Photo @person={{person}} />
            </div>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>
